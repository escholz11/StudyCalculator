      <ComboBox fx:id="studyCourseDropDown" layoutX="218.0" layoutY="318.0" onAction="#studyCourseAction" prefHeight="40.0" prefWidth="351.0" promptText="%studyCourse" style="-fx-background-color: #8B8B7A;" />
